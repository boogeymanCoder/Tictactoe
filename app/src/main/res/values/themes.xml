<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Razzmatazz" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_pink</item>
        <item name="colorPrimaryVariant">@color/razzmatazz</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/eucalyptus</item>
        <item name="colorSecondaryVariant">@color/medium_spring_green</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Button.Teal" parent="Widget.AppCompat.Button">
        <item name="android:backgroundTint">?attr/colorPrimary</item>
    </style>

